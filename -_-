            loadstring(game:HttpGet("https://raw.githubusercontent.com/fatesc/fates-admin/main/main.lua"))();
        end)
    
        GuisSection:NewButton("CMD - X", "A Gui with fly noclip and more", function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source",true))()
        end)
    
        GuisSection:NewButton("Dex", "Dex explorer is like roblox studio (you can see the code)", function()
            loadstring(game:HttpGet(('https://gist.githubusercontent.com/DinosaurXxX/b757fe011e7e600c0873f967fe427dc2/raw/ee5324771f017073fc30e640323ac2a9b3bfc550/dark%2520dex%2520v4'),true))()
        end)



    
       --Option

    local Option = Window:NewTab("Options")
    local OptionSection = Option:NewSection("Main")

    OptionSection:NewKeybind("Toggle UI", "Set Key bind to toggle the UI", Enum.KeyCode.RightAlt, function()
        Library:ToggleUI()
        end)
    end
end
