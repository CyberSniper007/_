        for k,v in pairs(game:GetService("Workspace").Water:GetChildren()) do
           v.CanCollide = true
           v.Anchored = true
           v.Material = "Ice"
        end
        else
            for k,v in pairs(game:GetService("Workspace").Water:GetChildren()) do
        v.CanCollide = false
           v.Anchored = false
           v.Material = "Ice"
        end
        end
    end)

 
