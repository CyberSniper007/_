    --// Lines for 3D box (12)
    local line1 = Drawing.new("Line")
    line1.Visible = false
    line1.Color = Box_Color
    line1.Thickness = Box_Thickness
    line1.Transparency = Box_Transparency

    local line2 = Drawing.new("Line")
