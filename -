if game.PlaceId == 1224212277 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Mad City Gui By Neko#4061", "BloodTheme")


    -- Mad City

    -- Main

    local Main = Window:NewTab("Scripts")
    local MainSection = Main:NewSection("Main")



    MainSection:NewButton("Aimbot", "Helps your Aim", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/SoldoxD/SoldoxD/main/Aimbot"))();
    end)
    

    MainSection:NewButton("Silent aim", "Helps your Aim", function()
        local uis = game:GetService("UserInputService")

            uis.InputBegan:Connect(function(asd)
                if asd.UserInputType == Enum.UserInputType.MouseButton1 then
                    for i,v in pairs(game.Workspace:GetChildren()) do
                        if v:FindFirstChild("Head") then
                        for i = 1,1 do
                        local args = {
                        [1] = "VR",
                        [2] = v.Head,
                        }

                        game:GetService("ReplicatedStorage").Event:FireServer(unpack(args))
                        end
                    end
                end
            end
        end)
    end)


    MainSection:NewButton("OP Silent aim", "Helps your Aim", function()
        local uis = game:GetService("UserInputService")

            uis.InputBegan:Connect(function(asd)
                if asd.UserInputType == Enum.UserInputType.MouseButton1 then
                    for i,v in pairs(game.Workspace:GetChildren()) do
                        if v:FindFirstChild("Head") then
                        for i = 1,5 do
                        local args = {
                        [1] = "VR",
                        [2] = v.Head,
                        }

                        game:GetService("ReplicatedStorage").Event:FireServer(unpack(args))
                        end
                    end
                end
            end
        end)
    end)



    MainSection:NewButton("One time Auto Rob", "Farmes you some cash $ ", function()
        _G.MaxPlayers = 7
        loadstring(game:HttpGet("https://raw.githubusercontent.com/ThisUsernameWasObfuscatedUsingPSUObf40A/Scripts/main/MadCityAutoRob"))()
    end)
   
    MainSection:NewButton("Auto Rob", "Farmes you some cash $ ", function()
        loadstring(game:HttpGet("https://system-exodus.com/scripts/madcity/MadLadsAR.lua",true))()
    end)
    
    MainSection:NewButton("Xp Farm", "Helps you level up (auto farm xp for you)", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/SoldoxD/SoldoxD/main/Textdokument%20(neu)%20(4).txt",true))()
    end)




    --Teleport/Robs
    local Tp = Window:NewTab("Teleport/Robs")
    local TpSection = Tp:NewSection("Main")


    TpSection:NewButton("Tp: Prison out", "-", function()
        local _speed=2500
        function tp(...)
           local plr=game.Players.LocalPlayer
           local args={...}
           if typeof(args[1])=="number"and args[2]and args[3]then
              args=Vector3.new(args[1],args[2],args[3])
           elseif typeof(args[1])=="Vector3" then
               args=args[1]    
           elseif typeof(args[1])=="CFrame" then
               args=args[1].Position
           end
           local dist=(plr.Character.HumanoidRootPart.Position-args).Magnitude
           game:GetService("TweenService"):Create(
               plr.Character.HumanoidRootPart,
               TweenInfo.new(dist/_speed,Enum.EasingStyle.Linear),
               {CFrame=CFrame.new(args)}
           ):Play()
        end
        
        tp(-893, 53, -2632)
    end)



